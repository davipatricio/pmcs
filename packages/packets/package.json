{
  "name": "@pmcs/packets",
  "version": "1.0.0",
  "description": "Easily create and parse Minecraft packets.",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc",
    "lint": "eslint . --cache --ext .ts"
  },
  "exports": {
    ".": {
      "import": "./dist/1.20.2.js",
      "require": "./dist/1.20.2.cjs"
    },
    "./1.8": {
      "import": "./dist/1.8.js",
      "require": "./dist/1.8.cjs"
    },
    "./1.20.2": {
      "import": "./dist/1.20.2.js",
      "require": "./dist/1.20.2.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/1.20.2.d.ts"
      ],
      "1.8": [
        "./dist/1.8.d.ts"
      ],
      "1.20.2": [
        "./dist/1.20.2.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "eslint": "^8.51.0",
    "eslint-config-neon": "^0.1.57",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@pmcs/chat": "workspace:^",
    "@pmcs/encoding": "workspace:^",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3"
  }
}

{
  "name": "@pmcs/packets",
  "version": "1.0.0",
  "description": "Easily create and parse Minecraft packets.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc",
    "lint": "biome check .",
    "format": "biome check --apply ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/versions/*.js",
      "require": "./dist/versions/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/versions/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@pmcs/chat": "workspace:^",
    "@pmcs/encoding": "workspace:^",
    "pino": "^8.18.0",
    "pino-pretty": "^11.0.0",
    "uuid": "^9.0.1"
  }
}

{
  "name": "@pmcs/packets",
  "version": "1.0.0",
  "description": "Easily create and parse Minecraft packets.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc",
    "lint": "eslint . --cache --ext .ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/versions/*.js",
      "require": "./dist/versions/*.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/versions/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "@types/uuid": "^9.0.6",
    "eslint": "^8.51.0",
    "eslint-config-neon": "^0.1.57",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@pmcs/chat": "workspace:^",
    "@pmcs/encoding": "workspace:^",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "uuid": "^9.0.1"
  }
}
